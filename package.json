{
  "name": "iamsophie.sk",
  "description": "Personal website of Sophie Shanahan-Kluth.",
  "version": "5.0.0",
  "scripts": {
    "build": "npx eleventy",
    "lint": "npx stylelint '**/*.scss'",
    "sass": "sass --style=compressed src/scss/main.scss _includes/css/main.css",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "debug": "DEBUG=* npx eleventy",
    "start": "npm-run-all sass lint --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamsophiesk/iamsophiesk.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamsophiesk/iamsophiesk/issues"
  },
  "homepage": "https://github.com/iamsophiesk/iamsophiesk#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "npm-run-all": "^4.1.5",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0"
  },
  "dependencies": {
    "clean-css": "^5.3.2",
    "sass": "^1.64.1"
  }
}
